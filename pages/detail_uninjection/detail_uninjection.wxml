<view class="content">
    <view class="col">
        <view class="title">就诊卡号</view>
        <view class="inp"><input type="text" value="{{medicalCard}}"/></view>
    </view>
    <view class="col">
        <view class="title">姓名</view>
        <view class="inp"><input type="text" value="{{name}}"/></view>
    </view>
    <view class="col">
        <view class="title">性别</view>
        <view class="inp"><input type="text" value="{{sex}}"/></view>
    </view>
    <view class="col">
        <view class="title">手机号</view>
        <view class="inp"><input type="text" value="{{tel}}"/></view>
    </view>
        <view class="col">
        <view class="title">预约项目</view>
        <view class="inp"><input type="text" value="{{project}}"/></view>
    </view>
    <view class="col">
        <view class="title">注射部位</view>
        <view class="inp"><input type="text" value='{{position}}'/></view>
    </view>
    <view class="col">
        <view class="title">注射剂量</view>
        <view class="inp"><input type="text" value='{{dose}}'/></view>
    </view>
    <view class="col">
        <view class="title">注射时间</view>
        <view class="inp"><input type="text" value="{{date}}"/></view>
    </view>
    <view class="col" wx:if="{{errorMsg === '成功' ? false : true}}">
        <view class="title">失败原因</view>
        <view class="inp"><textarea cols="10" rows="10" value='{{errorMsg}}'></textarea></view>
    </view>
</view>

