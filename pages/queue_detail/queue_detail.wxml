<view class="content">
    <view class="left_div"></view>
    <view class="right_div"></view>
    <view class="title">
        <view class="title_img">
            <image mode="widthFix" src="../../img/hospital.png"/>
        </view>
        <view class="title_name">
            中南大学湘雅二医院
        </view>
    </view>
    <view class="con_top">
        <view class="top_project">{{project}}</view>
        <view class="top_date">
            日期: <text>{{date}}</text>
        </view>
        <view class="top_date">
            需等待约: <text>{{cutDownTime}}分钟</text>
        </view>
        <view class="top_date">
            前方排队: <text>{{rank}}人</text>
        </view>
        <view class="top_date">
            号码: <text>{{scanNumber}}</text>
        </view>
    </view>
    <view class="con_bottom">
        <view class="btm">当前时间：{{nowTime}}</view>
        <view class="btm">检查时间：{{turnTime}}</view>
        <view class="btm_btn" bindtap="handleUnorder">取消检查</view>
    </view>
</view>
